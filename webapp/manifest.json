{
    "_version": "1.65.0",
    "sap.app": {
        "id": "logaligroup.logali",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.3",
            "toolsId": "9b1f74bd-c1e3-4d59-a789-46cf27c758cb"
        },
        "dataSources": {
            "Northwind.svc": {
                "uri": "/v3/northwind/northwind.svc/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "YSAPUI5_SRV_01":{
                "uri": "/sap/opu/odata/sap/YSAPUI5_SRV_01/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/YSAPUI5_SRV_01/metadata.xml"
                }                
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.132.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "logaligroup.logali.i18n.i18n"
                }
            },
            "odataNorthwind": {
                "dataSource": "Northwind.svc",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "OneWay",
                    "defaultOperationMode": "Server",
                    "defaultCountMode":"Request",
                    "refreshAfterChange": false
                }
            },
            "incidenceModel":{
                "type": "sap.ui.model.odata.v2.ODataModel",
                "dataSource": "YSAPUI5_SRV_01",
                "preload": true,
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Request",
                    "refreshAfterChange": false,
                    "useBatch": false,
                    "defaultUpdateMethod": "Put"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "logaligroup.logali.view",
                "async": true,
                "viewPath": "logaligroup.logali.view"
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern":"RouteMain",
                    "target": [
                        "TargetMain"
                    ]
                }
            ],
            "targets": {
                "TargetMain": {
                    "id": "Main",
                    "name": "Main"
                },
                "TargetMasterEmployee": {
                    "id": "MasterEmployee",
                    "name": "MasterEmployee"
                }
            }
        },
        "rootView": {
            "viewName": "logaligroup.logali.view.Main",
            "type": "XML",
            "id": "Main"
        }
    }
}